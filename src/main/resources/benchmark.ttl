@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix qb: <http://purl.org/linked-data/cube#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix hobbit: <http://w3id.org/hobbit/vocab#> .
@prefix exp: <http://w3id.org/hobbit/experiments#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bench: <http://w3id.org/bench#> .

# --- Benchmark ---

bench:Spatial a	hobbit:Benchmark;
	rdfs:label "Spatial Benchmark"@en;
	rdfs:comment	"WP4.1 HOBBIT Benchmark"@en;
# 	hobbit:imageName	"git.project-hobbit.eu:4567/jsaveta1/spatialbenchmarkcontroller";
 	hobbit:imageName	"spatial_benchmark-controller";
	hobbit:version "V1.1";
	hobbit:measuresKPI bench:precision ;
	hobbit:measuresKPI bench:recall ;
	hobbit:measuresKPI bench:fmeasure ;
	hobbit:hasParameter	bench:hasPopulation;
	hobbit:hasAPI	bench:SpatialApi;
	hobbit:hasParameter bench:SpatialDataFormat .

# --- metrics ---

bench:precision a hobbit:KPI;
rdfs:label "Precision"@en;
	rdfs:comment	"Precision = TP / (TP + FP)"@en;
	rdfs:range	xsd:float .

bench:recall a hobbit:KPI;
rdfs:label "Recall"@en;
	rdfs:comment	"Recall = TP / (TP + FN)"@en;
	rdfs:range	xsd:float .

bench:fmeasure a hobbit:KPI;
rdfs:label "F-measure"@en;
	rdfs:comment	"F-measure is the harmonic mean of precision and recall."@en;
	rdfs:range	xsd:float .
	
	
		
bench:SpatialApi a hobbit:API .

bench:hasPopulation	a	hobbit:Parameter, hobbit:FeatureParameter, hobbit:ConfigurableParameter;
        rdfs:label	"Population of generated data"@en;
        rdfs:comment	"Number of different events to be generated by a mimicking algorithm"@en;
        rdfs:domain	hobbit:Experiment, hobbit:Challenge;
        rdfs:range	xsd:unsignedInt;
        hobbit:defaultValue	"1000"^^xsd:unsignedInt .
        
# --- output data format ---

bench:SpatialDataFormat  a	hobbit:ConfigurableParameter;
	rdfs:label	"Spatial dataset format"@en;
	rdfs:comment	"Output data format."@en;
        rdfs:range bench:DataFormat;
	hobbit:hasDefaultValue bench:Turtle .

bench:DataFormat  a owl:Class ;
    rdfs:label	"Spatial dataset format"@en;
	rdfs:comment	"Output data format."@en .

bench:RDF_XML a bench:DataFormat ;
        rdfs:label	"RDF/XML dataset format"@en;
	rdfs:comment	"RDF/XML output data format."@en .

bench:N3 a bench:DataFormat ;
    rdfs:label	"N3 dataset format"@en;
	rdfs:comment	"N3 output data format."@en .

bench:Turtle a bench:DataFormat ;
    rdfs:label	"Turtle dataset format"@en;
	rdfs:comment	"Turtle output data format."@en .
	
bench:BinaryRDF a bench:DataFormat ;
    rdfs:label	"BinaryRDF dataset format"@en;
    rdfs:comment	"BinaryRDF output data format."@en .

bench:TriG a bench:DataFormat ;
    rdfs:label	"TriG dataset format"@en;
    rdfs:comment	"TriG output data format."@en .
    
bench:TriX a bench:DataFormat ;
    rdfs:label	"TriX dataset format"@en;
    rdfs:comment	"TriX output data format."@en .
    
bench:N_Triples a bench:DataFormat ;
    rdfs:label	"N-Triples dataset format"@en;
    rdfs:comment	"N-Triples output data format."@en .
    
bench:N_Quads a bench:DataFormat ;
    rdfs:label	"N-Quads dataset format"@en;
    rdfs:comment	"N-Quads output data format."@en .
    
bench:RDF_JSON a bench:DataFormat ;
    rdfs:label	"RDF/JSON dataset format"@en;
    rdfs:comment	"RDF/JSON output data format."@en .
    


